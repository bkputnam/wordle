{
  "name": "wordle",
  "version": "1.0.0",
  "description": "",
  "main": "src/main.ts",
  "scripts": {
    "build": "rm -rf bin/* && tsc",
    "test": "rm -rf bin/* && tsc && npx jasmine './bin/*_test.js'",
    "testDebug": "rm -rf bin/* && tsc && node --inspect-brk --enable-source-maps node_modules/jasmine/bin/jasmine.js 'bin/*_test.js'",
    "main": "rm -rf bin/* && tsc && node bin/main.js",
    "mainDebug": "rm -rf bin/* && tsc && node --inspect-brk --enable-source-maps bin/main.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jasmine": "^4.3.1",
    "@types/node": "^18.11.18",
    "jasmine": "^4.5.0",
    "typescript": "^4.9.4"
  }
}
